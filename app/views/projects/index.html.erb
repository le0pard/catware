<h1>Mobile App</h1>
<ul class="projects">
  <% @projects.each do |project| %>
    <li>
      <h2><%=link_to project.title, show_project_path(project), :class => "project_link" %></h2>
      <p><%=raw(project.short_description_markdown)%></p>
      <% if project.logo_img? %>
          <%=image_tag project.logo_img.url(:medium), :alt => project.title %>
      <% end %>
    </li>
  <% end %>
</ul>
